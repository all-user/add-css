    "babel-preset-es2016": "^6.0.11",
    "babel-preset-power-assert": "^1.0.0",
    "babelify": "^7.3.0",
    "browserify": "^9.0.3",
    "coffee-script": "^1.9.1",
    "eslint": "^2.9.0",
    "mocha": "^2.2.4",
    "npm-run-all": "^1.8.0",
