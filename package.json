  },
  "scripts": {
    "help": "npm-run-help",
    "test": "testem ci",
    "test:local": "testem",
    "build:browser": "browserify -t babelify -r ./src/index.js:append-css -o ./browser/build.js",
    "build:nodejs": "babel ./src -d ./lib",
